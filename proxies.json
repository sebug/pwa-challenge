{
    "proxies": {
        "staticFront": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/"
            },
            "backendUri": "https://pwachallenge.blob.core.windows.net/pwachallengestatic/index.html"
        },
	"otherFiles": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/static/{*restOfPath}"
            },
            "backendUri": "https://pwachallenge.blob.core.windows.net/pwachallengestatic/{restOfPath}"
        }
    }
}
